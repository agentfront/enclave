{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Enclave",
  "description": "Secure JavaScript sandbox for AI agent code execution. Protects against code injection, prototype pollution, and sandbox escapes.",
  "appearance": {
    "default": "dark",
    "strict": false
  },
  "theme": "venus",
  "favicon": {
    "light": "/favicon.ico",
    "dark": "/favicon.ico"
  },
  "colors": {
    "primary": "#e8a045",
    "light": "#f0b865",
    "dark": "#c78935",
    "background": {
      "dark": "#0a0908",
      "light": "#fdfbf7"
    }
  },
  "metadata": {
    "og:site_name": "Enclave Documentation",
    "og:image": "https://enclave.agentfront.dev/og-image.png",
    "og:locale": "en_US",
    "twitter:card": "summary_large_image",
    "twitter:site": "@agentfront",
    "twitter:creator": "@agentfront"
  },
  "navbar": {
    "links": [
      {
        "href": "https://enclave.agentfront.dev",
        "label": "Playground"
      },
      {
        "href": "https://github.com/agentfront/enclave",
        "icon": "github",
        "label": "GitHub"
      }
    ],
    "primary": {
      "href": "https://github.com/agentfront/enclave",
      "type": "github"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/agentfront/enclave",
      "x": "https://x.com/agentfront"
    }
  },
  "search": {
    "prompt": "Search Enclave docs..."
  },
  "seo": {
    "indexHiddenPages": false
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book",
        "versions": [
          {
            "version": "v1.0 (latest)",
            "default": true,
            "groups": [
              {
                "tag": "latest",
                "group": "Get Started",
                "pages": [
                  "docs/docs/getting-started/welcome",
                  "docs/docs/getting-started/installation"
                ]
              },
              {
                "group": "Libraries",
                "pages": [
                  "docs/docs/libraries/enclave",
                  "docs/docs/libraries/ast-guard",
                  "docs/docs/libraries/vectoriadb"
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
