{
  "name": "enclave-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/enclave-demo/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/enclave-demo",
        "main": "apps/enclave-demo/src/main.ts",
        "tsConfig": "apps/enclave-demo/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx apps/enclave-demo/src/main.ts"
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
