{
  "name": "enclave-vm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/enclave-vm/src",
  "projectType": "library",
  "tags": ["scope:libs", "scope:publishable"],
  "implicitDependencies": ["ast-guard"],
  "targets": {
    "test-perf": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/enclave-vm/jest.perf.config.ts",
        "runInBand": true
      }
    },
    "build-tsc": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/enclave-vm/dist",
        "main": "libs/enclave-vm/src/index.ts",
        "tsConfig": "libs/enclave-vm/tsconfig.lib.json",
        "assets": ["libs/enclave-vm/README.md", "libs/enclave-vm/SECURITY-AUDIT.md", "LICENSE"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-tsc"],
      "options": {
        "command": "node scripts/strip-dist-from-pkg.js libs/enclave-vm/dist/package.json"
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": ["build"],
      "options": {
        "packageRoot": "libs/enclave-vm/dist"
      }
    }
  }
}
