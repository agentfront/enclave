{
  "name": "enclavejs-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/enclavejs-client/src",
  "projectType": "library",
  "tags": ["scope:enclavejs", "type:lib"],
  "targets": {
    "build-tsc": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/enclavejs-client/dist",
        "main": "libs/enclavejs-client/src/index.ts",
        "tsConfig": "libs/enclavejs-client/tsconfig.lib.json",
        "assets": ["libs/enclavejs-client/*.md"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-tsc"],
      "options": {
        "command": "node scripts/strip-dist-from-pkg.js libs/enclavejs-client/dist/package.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/enclavejs-client"],
      "options": {
        "jestConfig": "libs/enclavejs-client/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
